<div class="panel panel-success swListPanel">
	<div class="panel-heading">
		<h2>List of all Organizations</h2> 
	</div>
<table class="table table-hover">
  <thead>
    <tr>
      <th>Organization Name</th>
      <th>Address</th>
      <th>Phone Number</th>
      <th>Show</th>
      <th>Edit</th>
      <th>Delete</th>
    </tr>
  </thead>

  <tbody>
    <% @organizations.each do |organization| %>
      <tr>
	<td> <%= organization.name %> </td>
	<td> <%= organization.address%> </td>
	<td> <%= organization.phone%> </td>
	<td> <%= link_to 'Show', organization, :class => "btn btn-success", data: { no_turbolink: true } %> </td>
	<td><%= link_to 'Edit', edit_organization_path(organization), :class => "btn btn-warning", data: { no_turbolink: true } %></td>
	<td><%= link_to 'Delete', organization, method: :delete, data: { confirm: 'Are you sure?' }, :class => "btn btn-danger" %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>
<br>

